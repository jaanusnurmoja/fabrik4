<?xml version="1.0" encoding="UTF-8"?>
<extension version="1.6" type="plugin" group="fabrik_form" method="upgrade">
	<name>Fabrik Form - autofill</name>
	<author>Rob Clayburn</author>
	<creationDate>{createdate}</creationDate>
	<copyright>(C) 2005-{year} fabrikar.com</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<authorEmail>rob@pollen-8.co.uk</authorEmail>
	<authorUrl>www.fabrikar.com</authorUrl>
	<version>{version}</version>
	<description>observers a specified field and looks up first matching record with the same data, then asks if you want to pre-fill in the form with that data</description>
	<files>
		<filename plugin="autofill">autofill.php</filename>
		<filename>index.html</filename>
		<filename>autofill.js</filename>
		<filename>autofill-min.js</filename>
		<folder>forms</folder>
		<folder>language</folder>
	</files>
	
</extension>