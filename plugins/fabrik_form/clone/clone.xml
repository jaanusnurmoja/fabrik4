<?xml version="1.0" encoding="UTF-8"?>
<extension version="1.6" type="plugin" group="fabrik_form" method="upgrade">
	<name>Fabrik Form - clone</name>
	<author>Hugh Messenger</author>
	<creationDate>Mar 2009</creationDate>
	<copyright>(C) 2005-2010 fabrikar.com</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<authorEmail>hugh.messenger@gmail.com</authorEmail>
	<authorUrl>www.fabrikar.com</authorUrl>
	<version>{version}</version>
	<description>Clones a submission X times</description>
	<files>
		<filename plugin="clone">clone.php</filename>
		<filename>index.html</filename>
		<folder>forms</folder>
	</files>

	<languages folder="language">
		<language tag="de-DE">de-DE/de-DE.plg_fabrik_form_clone.ini</language>
		<language tag="en-GB">en-GB/en-GB.plg_fabrik_form_clone.ini</language>
		<language tag="es-ES">es-ES/es-ES.plg_fabrik_form_clone.ini</language>
		<language tag="it-IT">it-IT/it-IT.plg_fabrik_form_clone.ini</language>
	</languages>
</extension>