	<name>Fabrik Visualization</name>
	<author>Rob Clayburn</author>
	<creationDate>August 2013</creationDate>
	<copyright>Copyright (C) 2005-2013 fabrikar.com - All rights reserved.</copyright>
	<license>GNU/GPL http://www.gnu.org/copyleft/gpl.html</license>
	<authorEmail>rob@pollen-8.co.uk</authorEmail>
	<authorUrl>www.fabrikar.com</authorUrl>
	<version>3.1b</version>
	<description>This module shows a Fabrik visualization in Joomlas adminstration pages.</description>
	<files>
		<filename module="mod_fabrik_visualizaton">mod_fabrik_visualizaton.php</filename>
		<filename>index.html</filename>
		<filename>mod_fabrik_visualizaton.xml</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset addfieldpath="/administrator/components/com_fabrik/models/fields" name="advanced">
				<field default="0" description="" label="Select Instance" name="id" type="visaulizationlist"/>
			</fieldset>
		</fields>
	</config>
</extension>
